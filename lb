! Filter Lists
filterlists.com#@#a:style(background-color: black !important)
filterlists.com#@#span:style(color: grey !important)
filterlists.com##span:style(color: #5bb1cd !important)
filterlists.com##span:style(color: #5bb1cd !important)
filterlists.com###text:style(background-color: grey !important)

! ColorHexa
colorhexa.com#@#li:style(background-color: black !important)
colorhexa.com#@#span:style(background-color: black !important)
colorhexa.com#@#a:style(background-color: black !important)
colorhexa.com##span.d:style(background-color: black !important)
colorhexa.com##span.t:style(background-color: black !important)

